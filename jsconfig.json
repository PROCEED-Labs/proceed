// configuration for IntelliSense for vscode
{
  "exclude": [
    "node_modules",
    "./src/management-system/dist/**/*",
    "dist",
    "**/node_modules/*",
    ".vscode",
    "build",
    "jsdoc",
  ],
  "compilerOptions": {
    "baseUrl": "./src", // this must be specified if "paths" is specified.
    "paths": {
      // defined aliases in webpack for import
      "@/*": ["management-system/src/*"],
    },
  },
}
